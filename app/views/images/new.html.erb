<h1>New image</h1>

<% form_for(@image, :html => { :multipart => true }) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :local_attachment, "Upload your image" %><br />
    <%= f.file_field :local_attachment %>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', images_path %>